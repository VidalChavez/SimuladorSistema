int main(void)
{
	
	int uno = 0b00000000;
	char dos = 0b00000000;
	
	
	

	printf("%X \n", uno);
	uno += 0b00010000;
	printf("%X \n", uno);
	
       
	
	printf("%x\n", dos);
	printf("%x\n", dos & 0xff);

}
